{
  "name": "graphics-production-panel",
  "version": "1.0.0",
  "cep": {
    "name": "Graphics Production Panel",
    "id": "graphics.production-panel",
    "hosts": "AEFT"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"yarn run start-js\" \"yarn run start-jsx\"",
    "start-js": "parcel src/js/index.html",
    "start-jsx": "parcel watch src/jsx/index.jsx.ts --no-hmr --no-source-maps",
    "build": "yarn run build-js && yarn run build-jsx",
    "build-js": "parcel build src/js/index.html --public-url .",
    "build-jsx": "parcel build src/jsx/index.jsx.ts --no-minify",
    "zxp": "create-zxp ./dist"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "fs-extra": "^6.0.1",
    "pino": "^4.16.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.2",
    "@types/pino": "^4.16.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-preset-cep": "^0.0.3",
    "babel-preset-extendscript": "^0.1.1",
    "babel-preset-react": "^6.24.1",
    "cep-interface": "^0.4.2",
    "concurrently": "^3.5.1",
    "create-zxp": "^0.1.0",
    "parcel-bundler": "^1.9.0",
    "parcel-plugin-cep": "^1.3.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "types-for-adobe": "pravdomil/types-for-adobe",
    "typescript": "^2.8.3",
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.4.1",
    "@material-ui/styles": "^4.4.1",
    "prop-types": "^15.7.2"
  }
}
